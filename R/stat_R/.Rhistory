library(dplyr)
library(ggplot2)
# Criar dados de exemplo
set.seed(123)
dados <- data.frame(valor = rnorm(100))
# Agrupar os valores em intervalos
dados <- dados %>%
mutate(intervalo = cut(valor, breaks = 5))
# Plotar o gráfico de barras
ggplot(dados, aes(x = intervalo)) +
geom_bar() +
scale_x_discrete(labels = function(x) {
# Obter os intervalos únicos
intervalos_unicos <- unique(dados$intervalo)
# Criar rótulos para os intervalos únicos
rótulos <- lapply(strsplit(intervalos_unicos, ","), function(x) paste0("[", x[1], ",", x[2], "]"))
# Retornar os rótulos correspondentes aos intervalos no gráfico
rótulos[seq_along(intervalos_unicos)]
})
